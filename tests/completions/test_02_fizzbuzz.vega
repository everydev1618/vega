// Test 02: FizzBuzz
// Print numbers 1-100, with Fizz/Buzz/FizzBuzz substitutions

fn main() {
    let i = 1;
    while i <= 100 {
        if i % 15 == 0 {
            print("FizzBuzz");
        } else if i % 3 == 0 {
            print("Fizz");
        } else if i % 5 == 0 {
            print("Buzz");
        } else {
            print(i);
        }
        i = i + 1;
    }
}
