// Test 10: Error Handling with Result
// Division with error handling

fn divide(a: int, b: int) -> Result<int, str> {
    if b == 0 {
        return Err("division by zero");
    }
    return Ok(a / b);
}

fn main() {
    let result1 = divide(10, 2);
    match result1 {
        Ok(value) => print(value),
        Err(msg) => print(msg)
    }

    let result2 = divide(10, 0);
    match result2 {
        Ok(value) => print(value),
        Err(msg) => print(msg)
    }
}
