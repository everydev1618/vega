// Test 03: Fibonacci (Recursive)
// Print first 20 Fibonacci numbers (0-indexed)

fn fib(n: int) -> int {
    if n <= 0 {
        return 0;
    }
    if n == 1 {
        return 1;
    }
    return fib(n - 1) + fib(n - 2);
}

fn main() {
    let i = 0;
    while i < 20 {
        print(fib(i));
        i = i + 1;
    }
}
