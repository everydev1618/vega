// Test 16: Prime Sieve
// Sieve of Eratosthenes to find primes up to 100

fn main() {
    // Initialize sieve array (1 = potentially prime)
    let is_prime: int[] = [];
    let i = 0;
    while i <= 100 {
        is_prime = is_prime + [1];
        i = i + 1;
    }

    // 0 and 1 are not prime
    is_prime[0] = 0;
    is_prime[1] = 0;

    // Sieve
    i = 2;
    while i * i <= 100 {
        if is_prime[i] == 1 {
            let j = i * i;
            while j <= 100 {
                is_prime[j] = 0;
                j = j + i;
            }
        }
        i = i + 1;
    }

    // Print primes
    i = 2;
    while i <= 100 {
        if is_prime[i] == 1 {
            print(i);
        }
        i = i + 1;
    }
}
